{
  "name": "mehr-it/levi-images",
  "description": "Image processing and optimization toolkit for Laravel",
  "license": "MIT",
  "keywords": [
    "laravel",
    "image",
    "manipulation",
    "processing",
    "imagine",
    "libvips",
    "vips",
    "imagick",
    "gmagick",
    "svg",
    "optimization",
    "optimizer",
    "jpegoptim",
    "optipng",
    "pngquant",
    "svgo",
    "gifsicle",
    "cwebp"
  ],
  "authors": [
    {
      "name": "Christopher Mieger",
      "email": "mail@mehr-it.info"
    }
  ],
  "autoload": {
    "psr-4": {
      "MehrIt\\LeviImages\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MehrItLeviImagesTest\\": "test/"
    }
  },
  "require": {
    "php": ">=7.3.0",
    "laravel/framework": "^5.8|^6.0|^7.0|^8.0",
    "spatie/image-optimizer": "^1.5",
    "imagine/imagine": "^1.2",
    "contao/imagine-svg": "^1.0",
    "ext-fileinfo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4|^8.4|^9.4",
    "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0",
    "mockery/mockery": "^1.4",
    "ext-imagick": "*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "MehrIt\\LeviImages\\Provider\\LeviImagesServiceProvider"
      ],
      "aliases": {
        "LeviImages": "MehrIt\\LeviImages\\Facades\\LeviImages"
      }
    }
  }
}
